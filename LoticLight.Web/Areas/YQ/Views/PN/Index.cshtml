@{
    ViewBag.Title = "PN";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
    var lst = ViewBag.lstGSM as List<LoticLight.Model.YqGSM>;
}
<script src="~/Content/lib/jqform/jquery.form.min.js"></script>
<link href="~/Content/lib/jqSelect2/css/select2.min.css" rel="stylesheet" />
<script src="~/Content/lib/bootstrap/bootstrap-dialog.js"></script>
<script src="~/Content/lib/jqSelect2/select2.js"></script>



<script src="@Url.VersionedContent("~/Content/Moulds/yq/PN.index.js")"></script>



<div id="div-change-gsm" style="display:none;" >
    <form>
        <table class="form">
            <tbody>
                <tr>
                    <td class="formTitle">Old GSM<font face="宋体">*</font></td>
                    <td class="formValue">
                        <select id="oldgsm" name="oldgsm" class="gsm ui-select2" isvalid="yes" checkexpession="NotNull">
                            <option></option>
                            @foreach (var gsm in lst)
                            {
                                <option>@gsm.Name</option>
                            }
                        </select>
                    </td>
                    <td class="formTitle">New GSM<font face="宋体">*</font></td>
                    <td class="formValue">
                        <select id="newgsm" name="newgsm" class="gsm ui-select2" isvalid="yes" checkexpession="NotNull">
                            <option></option>
                            @foreach (var gsm in lst)
                            {
                                <option>@gsm.Name</option>
                            }
                        </select>
                    </td>
                </tr>
            </tbody>
        </table>
    </form>
</div>
<div class="titlePanel">
    <div class="title-search">
        <table>
            <tr>
                <th></th>
                <td style="padding-left: 2px;">
                    <input id="txt_Keyword" type="text" class="form-control" style="width: 180px;" />
                </td>
                <td style="padding-left: 5px;">
                    <a id="btn_Search" class="btn btn-primary"><i class="fa fa-search"></i>&nbsp;查询</a>
                </td>

        </table>
    </div>
    <div class="toolbar">

        <div class="btn-group">
            <a id="btn-change-gsm" class="btn btn-default"><i class="fa fa-user"></i>&nbsp;Replace GSM</a>
           
        </div>
       
        <div class="btn-group">
            <a id="btn-upload" class="btn btn-default"><i class="fa fa-file-excel-o"></i>&nbsp;Upload</a>
            <a id="btn-export" class="btn btn-default" ><i class="fa fa-file-excel-o"></i>&nbsp;Download</a>
        </div>
        <div class="btn-group">
            <a id="btn-reload" class="btn btn-default" ><i class="fa fa-refresh"></i>&nbsp;ReQuery</a>
            
        </div>
        <div class="btn-group">

            <a id="btn-add" class="btn btn-default" "><i class="fa fa-plus"></i>&nbsp;Add</a>
            <a id="btn-edit" class="btn btn-default" ><i class="fa fa-pencil-square-o"></i>&nbsp;Edit</a>
            <a id="btn-delete" class="btn btn-default" ><i class="fa fa-trash-o"></i>&nbsp;Del</a>

        </div>
    </div>
</div>
<div class="gridPanel" style="width:90%">
    <table id="gridTable"></table>
    <div id="gridPager"></div>
</div>


